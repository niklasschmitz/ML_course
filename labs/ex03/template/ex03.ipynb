    "def ridge_regression(y, tx, lambda_):\n",
    "    \"\"\"implement ridge regression.\"\"\"\n",
    "    # ***************************************************\n",
    "    # INSERT YOUR CODE HERE\n",
    "    # ridge regression: TODO\n",
    "    # ***************************************************\n",
    "    N, M = tx.shape\n",
    "    w = np.linalg.inv(tx.T @ tx + lambda_*(2*N) * np.identity(M)) @ (tx.T @ y)\n",
    "    \n",
    "    return w"
   ]
